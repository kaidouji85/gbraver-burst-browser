{
  "name": "gbraver-burst-browser",
  "version": "1.0.0",
  "description": "frontend module for gbraver-burst.",
  "engines": {
    "node": "^16.18.1"
  },
  "main": "src/js/index.js",
  "scripts": {
    "start": "run-s clean dev-server",
    "start:production": "run-s build:production serve",
    "start:storybook": "start-storybook -s ./src/resources -p 6006",
    "start:storybook-static": "run-s clean build:storybook-static serve:storybook-static",
    "test": "jest",
    "build": "run-s clean type-check lint lint:test test webpack generate-icons scale-down-mobile-images webpack:sw webpack:clear-sw",
    "build:production": "run-s clean type-check lint lint:test test webpack:production generate-icons scale-down-mobile-images webpack:sw webpack:clear-sw",
    "build:storybook-static": "run-s clean lint:storybook storybook-static",
    "clean": "rimraf build",
    "dev-server": "webpack serve",
    "serve": "http-server build/production -p 3000 -c-1",
    "serve:storybook-static": "http-server build/storybook-static -c-1",
    "webpack": "webpack",
    "webpack:production": "webpack --mode=production",
    "webpack:sw": "webpack --config webpack.sw.js",
    "webpack:clear-sw": "webpack --config webpack.clear.sw.js",
    "storybook-static": "build-storybook -s ./src/resources -o build/storybook-static",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/js",
    "lint:test": "eslint test",
    "lint:storybook": "eslint stories",
    "lint:all": "run-s lint lint:test lint:storybook",
    "lint-fix": "eslint --fix src/js",
    "lint-fix:test": "eslint --fix test",
    "lint-fix:storybook": "eslint --fix stories",
    "lint-fix:all": "run-s lint-fix lint-fix:test lint-fix:storybook",
    "code-format": "prettier --write src/js",
    "code-format:test": "prettier --write test",
    "code-format:storybook": "prettier --write stories",
    "code-format:all": "run-s code-format code-format:test code-format:storybook",
    "code-analysis": "codehawk src/js",
    "scale-down-mobile-images": "node scale-down-mobile-images.js",
    "generate-icons": "node generate-icons.js"
  },
  "author": "y.takeuchi",
  "license": "ISC",
  "dependencies": {
    "@gbraver-burst-network/browser-core": "^1.6.2",
    "@gbraver-burst-network/browser-sdk": "^1.6.2",
    "@storybook/manager-webpack5": "^6.5.15",
    "@tweenjs/tween.js": "^18.6.4",
    "gbraver-burst-core": "1.16.3",
    "howler": "^2.2.3",
    "ramda": "^0.28.0",
    "rxjs": "^7.8.0",
    "stats.js": "^0.17.0",
    "three": "^0.148.0",
    "uuid": "^9.0.0",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "devDependencies": {
    "@storybook/builder-webpack5": "6.5.15",
    "@storybook/html": "^6.5.15",
    "@types/howler": "^2.2.7",
    "@types/jest": "^29.2.5",
    "@types/ramda": "^0.28.20",
    "@types/stats": "^0.16.30",
    "@types/three": "^0.148.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "codehawk-cli": "^10.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "image-size": "^1.0.2",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "2.8.2",
    "rimraf": "^3.0.2",
    "sharp": "^0.31.3",
    "style-loader": "^3.3.1",
    "to-ico": "^1.1.5",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
