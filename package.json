{
  "name": "gbraver-burst-browser",
  "version": "1.0.0",
  "description": "frontend module for gbraver-burst.",
  "engines": {
    "node": "^12.16.3"
  },
  "main": "src/js/index.js",
  "scripts": {
    "start": "npm-run-all clean dev-server",
    "start:production": "npm-run-all build:production serve",
    "start:storybook": "start-storybook -s ./src/resources -p 6006",
    "start:storybook-static": "npm-run-all clean build:storybook-static serve:storybook-static",
    "test": "ava",
    "build": "npm-run-all clean type-check webpack webpack:sw webpack:clear-sw",
    "build:production": "npm-run-all clean type-check webpack:production webpack:sw webpack:clear-sw",
    "build:storybook-static": "build-storybook -s ./src/resources -o build/storybook-static",
    "clean": "rimraf build",
    "dev-server": "webpack-dev-server",
    "serve": "http-server build/production -p 3000 -c-1",
    "serve:storybook-static": "http-server build/storybook-static",
    "webpack": "webpack",
    "webpack:production": "webpack --config webpack.production.js",
    "webpack:sw": "webpack --config webpack.sw.js",
    "webpack:clear-sw": "webpack --config webpack.clear.sw.js",
    "type-check": "flow check",
    "lint": "eslint src/js",
    "lint:test": "eslint test"
  },
  "author": "y.takeuchi",
  "license": "ISC",
  "dependencies": {
    "@tweenjs/tween.js": "^18.5.0",
    "gbraver-burst-core": "1.2.6",
    "puid": "^1.0.7",
    "ramda": "^0.27.0",
    "rxjs": "^6.5.5",
    "stats.js": "^0.17.0",
    "style-loader": "^1.2.1",
    "three": "^0.116.1",
    "uuid": "^8.1.0",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@storybook/html": "^5.3.19",
    "autoprefixer": "^9.8.0",
    "ava": "3.8.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-plugin-flowtype": "^5.1.0",
    "flow-bin": "^0.125.1",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "ava": {
    "babel": {
      "compileEnhancements": false
    },
    "files": [
      "test/js/**/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
