  "scripts": {
    "build": "run-s build:*",
    "build:clear-sw": "webpack --config webpack.clear.sw.js",
    "build:production": "webpack --mode=production",
    "build:storybook": "storybook build -o build/storybook",
    "build:sw": "webpack --config webpack.sw.js",
    "clean": "rimraf build",
    "code-analysis": "codehawk src/js",
    "code-format": "run-s code-format:*",
    "code-format:css": "prettier --write src/css",
    "code-format:helper-ts": "prettier --write *.ts",
    "code-format:src": "prettier --write src/js",
    "code-format:storybook": "prettier --write stories",
    "code-format:test": "prettier --write test",
    "detect-circular": "run-s detect-circular:*",
    "detect-circular:src": "madge --extensions ts -c src",
    "detect-circular:stories": "madge --extensions ts -c stories",
    "detect-circular:test": "madge --extensions ts -c test",
    "generate-icons": "ts-node generate-icons.ts",
    "lint": "run-s lint:src lint:*",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:css": "stylelint --fix --max-warnings 0 src/css/**/*.css",
    "lint-fix:helper-ts": "eslint --fix --max-warnings 0 *.ts",
    "lint-fix:src": "eslint --fix --max-warnings 0 src/js",
    "lint-fix:storybook": "eslint --fix --max-warnings 0 stories",
    "lint-fix:test": "eslint --fix --max-warnings 0 test",
    "lint:css": "stylelint --max-warnings 0 src/css/**/*.css",
    "lint:helper-ts": "eslint --max-warnings 0 *.ts",
    "lint:src": "eslint --max-warnings 0 src/js",
    "lint:storybook": "eslint --max-warnings 0 stories",
    "lint:test": "eslint --max-warnings 0 test",
    "scale-down-mobile-images": "ts-node scale-down-mobile-images.ts",
    "serve": "http-server build/production -p 3000 -c-1",
    "serve-storybook": "http-server build/storybook -c-1",
    "start": "webpack serve",
    "start-storybook": "storybook dev -p 6006",
    "test": "jest",
    "type-check": "tsc --noEmit"
  }
}
