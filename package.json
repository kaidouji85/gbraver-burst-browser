  "scripts": {
    "build": "run-s webpack generate-icons scale-down-mobile-images webpack:sw webpack:clear-sw",
    "build:production": "run-s webpack:production generate-icons scale-down-mobile-images webpack:sw webpack:clear-sw",
    "build:storybook-static": "storybook build -o build/storybook-static",
    "clean": "rimraf build",
    "code-analysis": "codehawk src/js",
    "code-format": "run-s code-format:src code-format:test code-format:storybook code-format:css code-format:helper-ts",
    "code-format:css": "prettier --write src/css",
    "code-format:helper-ts": "prettier --write *.ts",
    "code-format:src": "prettier --write src/js",
    "code-format:storybook": "prettier --write stories",
    "code-format:test": "prettier --write test",
    "detect-circular": "run-s detect-circular:src detect-circular:test detect-circular:stories",
    "detect-circular:src": "madge --extensions ts -c src",
    "detect-circular:test": "madge --extensions ts -c test",
    "detect-circular:stories": "madge --extensions ts -c stories",
    "generate-icons": "ts-node generate-icons.ts",
    "lint": "run-s lint:src lint:storybook lint:test lint:css lint:helper-ts",
    "lint-fix": "run-s lint-fix:src lint-fix:storybook lint-fix:test lint-fix:css lint-fix:helper-ts",
    "lint-fix:css": "stylelint --fix src/css/**/*.css",
    "lint-fix:helper-ts": "eslint --fix *.ts",
    "lint-fix:src": "eslint --fix src/js",
    "lint-fix:storybook": "eslint --fix stories",
    "lint-fix:test": "eslint --fix test",
    "lint:css": "stylelint src/css/**/*.css",
    "lint:helper-ts": "eslint *.ts",
    "lint:src": "eslint src/js",
    "lint:storybook": "eslint stories",
    "lint:test": "eslint test",
    "scale-down-mobile-images": "ts-node scale-down-mobile-images.ts",
    "serve": "http-server build/production -p 3000 -c-1",
    "serve:storybook-static": "http-server build/storybook-static -c-1",
    "start": "webpack serve",
    "start:production": "run-s build:production serve",
    "start:storybook": "storybook dev -p 6006",
    "start:storybook-static": "run-s build:storybook-static serve:storybook-static",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "webpack": "webpack",
    "webpack:clear-sw": "webpack --config webpack.clear.sw.js",
    "webpack:production": "webpack --mode=production",
    "webpack:sw": "webpack --config webpack.sw.js"
  }
}
