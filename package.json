{
  "name": "gbraver-burst-browser",
  "version": "1.0.0",
  "description": "frontend module for gbraver-burst.",
  "engines": {
    "node": "^16.17.0"
  },
  "main": "src/js/index.js",
  "scripts": {
    "start": "run-s clean dev-server",
    "start:production": "run-s build:production serve",
    "start:storybook": "start-storybook -s ./src/resources -p 6006",
    "start:storybook-static": "run-s clean build:storybook-static serve:storybook-static",
    "test": "jest",
    "build": "run-s clean type-check lint lint:test webpack generate-icons scale-down-mobile-images webpack:sw webpack:clear-sw",
    "build:production": "run-s clean type-check lint lint:test webpack:production generate-icons scale-down-mobile-images webpack:sw webpack:clear-sw",
    "build:storybook-static": "run-s clean lint:storybook storybook-static",
    "clean": "rimraf build",
    "dev-server": "webpack serve",
    "serve": "http-server build/production -p 3000 -c-1",
    "serve:storybook-static": "http-server build/storybook-static -c-1",
    "webpack": "webpack",
    "webpack:production": "webpack --mode=production",
    "webpack:sw": "webpack --config webpack.sw.js",
    "webpack:clear-sw": "webpack --config webpack.clear.sw.js",
    "storybook-static": "build-storybook -s ./src/resources -o build/storybook-static",
    "type-check": "flow check",
    "lint": "eslint src/js",
    "lint:test": "eslint test",
    "lint:storybook": "eslint stories",
    "scale-down-mobile-images": "node scale-down-mobile-images.js",
    "generate-icons": "node generate-icons.js"
  },
  "author": "y.takeuchi",
  "license": "ISC",
  "dependencies": {
    "@gbraver-burst-network/browser-core": "^1.5.7",
    "@gbraver-burst-network/browser-sdk": "^1.5.7",
    "@storybook/manager-webpack5": "^6.5.10",
    "@tweenjs/tween.js": "^18.6.4",
    "gbraver-burst-core": "1.12.2",
    "howler": "^2.2.3",
    "ramda": "^0.28.0",
    "rxjs": "^7.5.6",
    "stats.js": "^0.17.0",
    "three": "^0.144.0",
    "uuid": "^9.0.0",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-flow": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/html": "^6.5.10",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^27.0.3",
    "flow-bin": "^0.186.0",
    "flow-typed": "^3.8.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "image-size": "^1.0.2",
    "jest": "^29.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.1",
    "rimraf": "^3.0.2",
    "sharp": "^0.31.0",
    "style-loader": "^3.3.1",
    "to-ico": "^1.1.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  }
}
