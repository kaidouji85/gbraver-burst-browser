    "code-format:helper-js": "prettier --write *.js",
    "code-format:helper-ts": "prettier --write *.ts",
    "code-format:src": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "code-format:storybook": "prettier --write stories",
    "code-format:test": "prettier --write test",
    "dependency-check": "run-s dependency-check:*",
    "dependency-check:src": "depcruise src",
    "dependency-check:stories": "depcruise stories",
    "dependency-check:test": "depcruise test",
    "generate-icons": "ts-node --project tsconfig.node.json generate-icons.ts",
    "lint": "run-s lint:*",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:css": "stylelint --fix --max-warnings 0 \"src/**/*.css\"",
    "lint-fix:hbs": "eslint --fix --max-warnings 0 -c eslint.hbs.config.js src/js",
    "lint-fix:helper-ts": "eslint --fix --max-warnings 0 *.ts",
    "lint-fix:src": "eslint --fix --max-warnings 0 \"src/**/*.ts\" \"src/**/*.js\"",
    "lint-fix:storybook": "eslint --fix --max-warnings 0 stories",
    "lint-fix:test": "eslint --fix --max-warnings 0 test",
    "lint:css": "stylelint --max-warnings 0 \"src/**/*.css\"",
    "lint:hbs": "eslint --max-warnings 0 -c eslint.hbs.config.js src/js",
    "lint:helper-ts": "eslint --max-warnings 0 *.ts",
    "lint:src": "eslint --max-warnings 0 \"src/**/*.ts\" \"src/**/*.js\"",
    "lint:storybook": "eslint --max-warnings 0 stories",
    "lint:test": "eslint --max-warnings 0 test",
    "scale-down-mobile-images": "ts-node --project tsconfig.node.json scale-down-mobile-images.ts",
